
@{
    ViewBag.Title = "DanhSachNguoiDung";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}


<div class="row">
    <div class="col-12">
        <div class="card">
            <nav aria-label="breadcrumb ">
                <ol class="breadcrumb ">
                    <li class="breadcrumb-item">
                        <a class="black-text showload" href="/Admin/Admin/Index">Home</a><i class="fas fa-angle-right mx-2"></i>
                    </li>
                    Users
                </ol>
            </nav>
            <div class="card-body">
                <div class="col-md-12  waves-light text-uppercase">
                    <h3 class="text-center display-5">
                        Users
                    </h3>
                    <hr />
                </div>
                <div class="table-responsive text-center">
                    <div id="data"></div>
                    
                </div>
            </div>
        </div>
    </div>
</div>

<div class="footer-table">
    <div class="row">
        <div class="col-md-3 m-4">
            <button class="btn btn-info btn-sm  waves-effect waves-light" id="SelectDelete" onclick="ResetPass('/Admin/QLNguoiDung/ResetPass', '/Admin/QlNguoiDung/_DanhSachNguoiDung')">
                <i class=" icon-reload"></i> Reset Password
            </button>
            <label class="non-touch" id="selected-row">
                <span class="checkbox-material">
                    <span class="check-count"></span>
                </span>Selected <span id="numberOfSelectedRows">0</span> lines
            </label>
        </div>

        <div class="col-md-8 m-4  row">
            <div class="col-md-8">
                <div class="pull-right">
                    <label class="non-touch " id="selected-row">
                        <a href="~/Areas/Admin/Content/Excel/NguoiDung.xls">[ <i class="fas fa-download"></i> File Excel ]</a>
                    </label>
                    <span class="btn btn-success btn-sm  waves-effect waves-light btn-file">
                        <i class="fas fa-plus-square"></i>  ImportExcel
                        <input type="file" id="ImportEx" onchange="ImportEx('/Admin/QLNguoiDung/ImportExcel','/Admin/QLNguoiDung/_DanhSachExcel')" accept=".csv, application/vnd.openxmlformats-officedocument.spreadsheetml.sheet, application/vnd.ms-excel">
                    </span>
                </div>

            </div>
            <div class="col-md-2 ">
                <button class="btn btn-success btn-sm  waves-effect waves-light " onclick="ModalThemAdmin('/Admin/QLNguoiDung/_ThemNguoiDung')">
                    <i class="fas fa-plus-square"></i> ADD
                </button>
            </div>
        </div>

    </div>
</div>

@section Scripts{
    <script src="~/Areas/Admin/Content/AdminJs/QLNguoiDungJS.js"></script>
}




