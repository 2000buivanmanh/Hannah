


@model DMSS.ViewModals.DsExcelViewModal.ExcelNguoiDung

@{
    var DsThatbai = ViewBag.DsThatbai;
    var DsThanhCong = ViewBag.DsThanhCong;
}

<div class="modal-body">
    <div class="text-center mt-2 mb-4">
        <h2 id="tieude">
            Import Excel
        </h2>
    </div>

    <form class="pl-5 pr-5" action="#" id="frmThemOrSua">
        <h3> Error List</h3>
        <div class="table-responsive text-center">
            <div id="dataEx">
                <table id="dtable" class="table table-striped table-bordered no-wrap" style="width:100%">
                    <thead>
                        <tr>
                            <th> @Html.DisplayNameFor(model => model.Stt)</th>
                            <th> @Html.DisplayNameFor(model => model.TenDangNhap)</th>
                            <th> @Html.DisplayNameFor(model => model.EmailNguoiDung)</th>
                            <th> @Html.DisplayNameFor(model => model.HoTen)</th>
                            <th> @Html.DisplayNameFor(model => model.ThongBao)</th>

                        </tr>
                    </thead>

                    <tbody>
                        @foreach (var item in DsThatbai)
                        {
                            <tr>
                                <td>
                                    @item.Stt
                                </td>
                                <td>
                                    @item.TenDangNhap
                                </td>
                                <td>
                                    @item.EmailNguoiDung
                                </td>
                                <td>
                                    @item.HoTen
                                </td>
                                <td>
                                    @item.ThongBao
                                </td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
        <h3>
            Success list
        </h3>
        <div class="table-responsive text-center">
            <div id="dataEx">
                <table id="dtable" class="table table-striped table-bordered no-wrap" style="width:100%">
                    <thead>
                        <tr>
                            <th> @Html.DisplayNameFor(model => model.Stt)</th>
                            <th> @Html.DisplayNameFor(model => model.TenDangNhap)</th>
                            <th> @Html.DisplayNameFor(model => model.EmailNguoiDung)</th>
                            <th> @Html.DisplayNameFor(model => model.HoTen)</th>
                            <th> @Html.DisplayNameFor(model => model.ThongBao)</th>

                        </tr>
                    </thead>

                    <tbody>
                        @foreach (var item in DsThanhCong)
                        {
                            <tr>
                                <td>
                                    @item.Stt
                                </td>
                                <td>
                                    @item.TenDangNhap
                                </td>
                                <td>
                                    @item.EmailNguoiDung
                                </td>
                                <td>
                                    @item.HoTen
                                </td>
                                <td>
                                    @item.ThongBao
                                </td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>

        <div class="form-group text-center">
            <div class="btn btn-primary" id="btnsumit" onclick="ImportExcel('/Admin/QLNguoiDung/AddListExcel','/Admin/QLNguoiDung/_DanhSachNguoiDung')">
                Import Excel
            </div>
        </div>
    </form>

</div>






