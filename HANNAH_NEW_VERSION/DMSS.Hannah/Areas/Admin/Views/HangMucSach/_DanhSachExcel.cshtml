

@model DMSS.ViewModals.DsExcelViewModal.ExcelHangMucSach
@{
    var DsThatbai = ViewBag.DsThatbai;
    var DsThanhCong = ViewBag.DsThanhCong;
}

<div class="modal-body">
    <div class="text-center mt-2 mb-4">
        <h2 id="tieude">
            Import Excel
        </h2>
    </div>

    <form class="pl-5 pr-5" action="#" id="frmThemOrSua">
        <h3> Error List</h3>
        <div class="table-responsive text-center">
            <div id="dataEx">
                <table id="dtable" class="table table-striped table-bordered no-wrap" style="width:100%">
                    <thead>
                        <tr>
                            <th> @Html.DisplayNameFor(model => model.Stt)</th>
                            <th> @Html.DisplayNameFor(model => model.TenHangMuc)</th>
                            <th> @Html.DisplayNameFor(model => model.MaNhanDienHangMucSach)</th>
                            <th> @Html.DisplayNameFor(model => model.Mota)</th>
                            <th> @Html.DisplayNameFor(model => model.NoiDungSeo)</th>
                            <th> @Html.DisplayNameFor(model => model.TuKhoaSeo)</th>
                            <th> @Html.DisplayNameFor(model => model.TieuDeSeo)</th>
                            <th> @Html.DisplayNameFor(model => model.DuongDanSeo)</th>
                            <th> @Html.DisplayNameFor(model => model.ThongBao)</th>

                        </tr>
                    </thead>

                    <tbody>
                        @foreach (var item in DsThatbai)
                        {
                        <tr>
                            <td>
                                @item.Stt
                            </td>
                            <td>
                                @item.TenHangMuc
                            </td>
                            <td>
                                @item.MaNhanDienHangMucSach
                            </td>
                            <td>
                                @item.Mota
                            </td>
                            <td>
                                @item.NoiDungSeo

                            </td>
                            <td>
                                @item.TuKhoaSeo
                            </td>
                            <td>
                                @item.TieuDeSeo
                            </td>

                            <td>
                                @item.DuongDanSeo
                            </td>
                            <td>
                                @item.ThongBao
                            </td>
                        </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
        <h3>
            Success list
        </h3>
        <div class="table-responsive text-center">
            <div id="dataEx">
                <table id="dtable" class="table table-striped table-bordered no-wrap" style="width:100%">
                    <thead>
                        <tr>
                            <th> @Html.DisplayNameFor(model => model.Stt)</th>
                            <th> @Html.DisplayNameFor(model => model.TenHangMuc)</th>
                            <th> @Html.DisplayNameFor(model => model.MaNhanDienHangMucSach)</th>
                            <th> @Html.DisplayNameFor(model => model.Mota)</th>
                            <th> @Html.DisplayNameFor(model => model.NoiDungSeo)</th>
                            <th> @Html.DisplayNameFor(model => model.TuKhoaSeo)</th>
                            <th> @Html.DisplayNameFor(model => model.TieuDeSeo)</th>
                            <th> @Html.DisplayNameFor(model => model.DuongDanSeo)</th>
                            <th> @Html.DisplayNameFor(model => model.ThongBao)</th>

                        </tr>
                    </thead>

                    <tbody>
                        @foreach (var item in DsThanhCong)
                        {
                        <tr>
                            <td>
                                @item.Stt
                            </td>
                            <td>
                                @item.TenHangMuc
                            </td>
                            <td>
                                @item.MaNhanDienHangMucSach
                            </td>
                            <td>
                                @item.Mota
                            </td>
                            <td>
                                @item.NoiDungSeo

                            </td>
                            <td>
                                @item.TuKhoaSeo
                            </td>
                            <td>
                                @item.TieuDeSeo
                            </td>

                            <td>
                                @item.DuongDanSeo
                            </td>
                            <td>
                                @item.ThongBao
                            </td>
                        </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>

        <div class="form-group text-center">
            <div class="btn btn-primary" id="btnsumit" onclick="ImportExcel('/Admin/HangMucSach/AddListExcel','/Admin/HangMucSach/_DanhSachHangMucSach')">
                Import Excel
            </div>
        </div>
    </form>

</div>




